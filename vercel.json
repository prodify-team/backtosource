{
  "version": 2,
  "functions": {
    "api/chat.js": {
      "runtime": "@vercel/node"
    },
    "api/chat-simple.js": {
      "runtime": "@vercel/node"
    },
    "api/test.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  }
}